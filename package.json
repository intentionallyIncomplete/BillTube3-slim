{
  "name": "billtube3-slim",
  "version": "3.4.0",
  "description": "Modular UI enhancement framework for CyTube",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "lint-staged": "eslint . --ext .js,.css,.md,.json",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:css -- --fix",
    "build": "node scripts/build.js",
    "purge-cdn": "curl -s -X GET \"https://purge.jsdelivr.net/gh/intentionallyIncomplete/BillTube3-slim@dev/channel_config_settings.js\" && echo \"âœ“ CDN cache purged\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intentionallyIncomplete/BillTube3-slim.git"
  },
  "author": "Quigly",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "semantic-release": "^24.2.9",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "terser": "^5.44.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/github",
        {
          "assets": []
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
